; Sorts
(declare-sort ElementSort 0)
(declare-sort ArraySort 0)

; Function declarartions
(declare-fun diff (ArraySort ArraySort) Int)
(declare-fun wr (ArraySort Int ElementSort) ArraySort)
(declare-fun rd (ArraySort Int) ElementSort)

(declare-fun a () ArraySort)
(declare-fun b () ArraySort)
(declare-fun j () Int)
(declare-fun i1 () Int)
(declare-fun i2 () Int)
(declare-fun i3 () Int)
(declare-fun i4 () Int)
(declare-fun i5 () Int)
(declare-fun i6 () Int)
(declare-fun i7 () Int)
(declare-fun i8 () Int)
(declare-fun e1 () ElementSort)
(declare-fun e2 () ElementSort)
(declare-fun e3 () ElementSort)
(declare-fun e4 () ElementSort)
(declare-fun e5 () ElementSort)
(declare-fun e6 () ElementSort)
(declare-fun e7 () ElementSort)
(declare-fun e8 () ElementSort)
(declare-fun p1 (Int) Bool)
(declare-fun p2 (Int) Bool)
(declare-fun p3 (Int) Bool)
(declare-fun p4 (Int) Bool)
(declare-fun p5 (Int) Bool)
(declare-fun p6 (Int) Bool)
(declare-fun p7 (Int) Bool)
(declare-fun p8 (Int) Bool)

(assert 
(and
(= a (wr (wr (wr (wr (wr (wr (wr b i7 e7) i6 e6) i5 e5) i4 e4) i3 e3) i2 e2) i1 e1))
(= (p1 i1) true)
(= (p2 i2) true)
(= (p3 i3) true)
(= (p4 i4) true)
(= (p5 i5) true)
(= (p6 i6) true)
(= (p7 i7) true)
(= (p8 i8) true)
)
)

(assert 
(and 
(distinct (rd a j) (rd b j))
(= (p1 j) false)
(= (p2 j) false)
(= (p3 j) false)
(= (p4 j) false)
(= (p5 j) false)
(= (p6 j) false)
(= (p7 j) false)
(= (p8 j) false)
)
)

(check-sat)
